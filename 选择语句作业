#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
int main()
{
	int score;
		printf("请输入学生成绩: \n");
		scanf("%d", &score);
	if (score >= 90 && score <= 100)
	{
		printf("A级\n");
	}
	else if (score >= 75 && score <= 89)
	{
		printf("B级\n");
	}
	else if (score >= 60 && score <= 74)
	{
		printf("C级\n");
	}
	else if (score >= 0 && score <= 59)
	{
		printf("D级\n");
	}
	return 0;
}
